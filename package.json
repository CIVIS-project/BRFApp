{
  "name": "brf-energi",
  "version": "2.0.0",
  "description": "BRF Energi app backend",
  "main": "backend/server.js",
  "scripts": {
    "start": "node backend/server.js",
    "restart": "nodemon backend/server.js -i ./frontend",
    "test": "cd backend && NODE_ENV=test MONGOOSE_DISABLE_STABILITY_WARNING=1 mocha",
    "coverage": "cd backend && NODE_ENV=test MONGOOSE_DISABLE_STABILITY_WARNING=1 istanbul cover _mocha -- -R spec",
    "build:js": "browserify backend/index.js -g unassertify -t localenvify -t yo-yoify -t [ babelify --presets [ babel-preset-es2015 ] ] -g uglifyify | uglifyjs -o backend/public/index.js",
    "build:css": "postcss backend/index.css -u postcss-import -u postcss-custom-media -u postcss-custom-properties -u autoprefixer -u postcss-csso -o backend/public/index.css"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CIVIS-project/BRFEnergi"
  },
  "license": "Apache-2.0",
  "engines": {
    "node": ">=0.6.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.2",
    "babel-preset-es2015": "^6.22.0",
    "babelify": "^7.3.0",
    "browserify": "^14.0.0",
    "chai": "^2.3.0",
    "coveralls": "^2.11.2",
    "eslint": "^3.12.2",
    "gulp": "^3.8.11",
    "gulp-apidoc": "^0.1.3",
    "istanbul": "^0.3.14",
    "jscs": "^1.13.1",
    "localenvify": "^1.0.1",
    "mocha-jscs": "^1.1.0",
    "mocha-jshint": "^2.2.3",
    "postcss": "^5.2.11",
    "postcss-csso": "^1.1.2",
    "postcss-custom-media": "^5.0.1",
    "postcss-custom-properties": "^5.0.2",
    "postcss-import": "^9.1.0",
    "suitcss-base": "^2.0.0",
    "suitcss-components-button": "^6.0.0",
    "suitcss-components-grid": "^3.0.3",
    "suitcss-utils-display": "^1.0.2",
    "suitcss-utils-flex": "^1.1.1",
    "suitcss-utils-layout": "^1.0.1",
    "suitcss-utils-link": "^1.0.1",
    "suitcss-utils-size": "^2.0.0",
    "suitcss-utils-space": "^0.4.1",
    "suitcss-utils-text": "^1.0.0",
    "uglify-js": "^2.7.5",
    "uglifyify": "^3.0.4",
    "unassertify": "^2.0.4",
    "watchify-middleware": "^1.6.0",
    "yo-yoify": "^3.5.0"
  },
  "dependencies": {
    "async": "^1.3.0",
    "body-parser": "~1.12.0",
    "choo": "^4.1.0",
    "colors": "^1.1.2",
    "compression": "^1.6.2",
    "cookie-parser": "~1.3.3",
    "csv": "^0.4.6",
    "dateformat": "^1.0.12",
    "dedent": "^0.6.0",
    "dotenv": "^2.0.0",
    "email-templates": "^2.0.1",
    "escape-string-regexp": "^1.0.3",
    "express": "^4.12.3",
    "express-session": "^1.15.0",
    "express-validator": "^2.10.0",
    "i18n": "^0.5.0",
    "moment": "^2.11.2",
    "mongoose": "^4.0.5",
    "nodemailer": "^1.4.0",
    "nodemon": "^1.4.0",
    "passport": "^0.2.1",
    "passport-http": "^0.2.2",
    "passport-local-mongoose": "^1.0.0",
    "passport-oauth2": "^1.4.0",
    "request": "^2.55.0",
    "underscore": "^1.8.3",
    "xml2js": "^0.4.8",
    "yargs": "^3.15.0"
  }
}
